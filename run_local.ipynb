    "                return\n",
    "            \n",
    "            # Calculate cumulative profit\n",
    "            settled_bets['cumulative_profit'] = settled_bets['profit_loss'].cumsum()\n",
    "            settled_bets['roi_pct'] = (settled_bets['cumulative_profit'] / CONFIG['bankroll']) * 100\n",
    "            \n",
    "            # Create chart\n",
    "            plt.figure(figsize=(12, 8))\n",
    "            plt.subplot(2, 2, 1)\n",
