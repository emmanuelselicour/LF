    "class GPTBetFootAnalyzer:\n",
    "    \"\"\"Main analysis workflow for GPT-Bet.Foot\"\"\"\n",
    "    \n",
    "    def __init__(self):\n",
    "        self.ocr = odds_ocr\n",
    "        self.trader = paper_trader\n",
    "        self.parser = data_parser\n",
    "    \n",
    "    def analyze_match(self, match_data: Dict) -> Dict:\n",
    "        \"\"\"Complete match analysis workflow\"\"\"\n",
    "        \n",
    "        # Add human-like delay\n",
    "        delay = random.uniform(*CONFIG['human_delay'])\n",
    "        time.sleep(delay)\n",
    "        \n",
    "        try:\n",
    "            # Extract match data\n",
    "            league = match_data['league']\n",
    "            home_team = match_data['home_team']\n",
    "            away_team = match_data['away_team']\n",
",
    "            # Get bookmaker odds\n",
    "            if 'screenshot_path' in match_data:\n",
    "                book_odds = self.ocr.extract_odds(match_data['screenshot_path'])\n",
    "            else:\n",
    "                book_odds = match_data['book_odds']\n",
    "            \n",
    "            # Calculate fair odds\n",
    "            delta_xg = match_data.get('delta_xg', 0)\n",
    "            delta_ranking = match_data.get('delta_ranking', 0)\n",
    "            delta_days = match_data.get('delta_days', 0)\n",
    "            \n",
    "            fair_odds_result = fair_odds(delta_xg, delta_ranking, delta_days)\n",
    "            \n",
    "            # Check for value bets\n",
    "            recommendations = []\n",
    "            \n",
    "            for outcome in ['home', 'draw', 'away']:\n",
    "                fair_odd = fair_odds_result['fair_odds'][outcome]\n",
    "                book_odd = book_odds[outcome]\n",
    "                \n",
    "                if book_odd > 0:\n",
    "                    edge = calculate_edge(fair_odd, book_odd)\n",
    "                    \n",
    "                    if abs(edge) > CONFIG['min_edge']:\n",
    "                        # Calculate Kelly stake\n",
    "                        prob = fair_odds_result['probabilities'][outcome]\n",
    "                        kelly_result = kelly(edge, prob, CONFIG['kelly_fraction'])\n",
    "                        \n",
    "                        if kelly_result['kelly_percentage'] > 0:\n",
    "                            recommendation = {\n",
    "                                'league': league,\n",
    "                                'home_team': home_team,\n",
    "                                'away_team': away_team,\n",
    "                                'selection': outcome.title(),\n",
    "                                'book_odds': book_odd,\n",
    "                                'fair_odds': fair_odd,\n",
    "                                'edge': edge * 100,\n",
    "                                'kelly_pct': kelly_result['kelly_percentage'],\n",
    "                                'probability': prob,\n",
    "                                'clv': edge * 100  # Simplified CLV\n",
    "                            }\n",
    "                            \n",
    "                            recommendations.append(recommendation)\n",
    "            \n",
    "            return {\n",
    "                'match_info': {\n",
    "                    'league': league,\n",
    "                    'home_team': home_team,\n",
    "                    'away_team': away_team,\n",
    "                    'fair_odds': fair_odds_result,\n",
    "                    'book_odds': book_odds\n",
    "                },\n",
    "                'recommendations': recommendations,\n",
    "                'timestamp': datetime.now().isoformat()\n",
    "            }\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Error in analysis: {e}\")\n",
    "            return {}\n",
    "    \n",
    "    def execute_bets(self, analysis_result: Dict):\n",
    "        \"\"\"Execute paper trades based on analysis\"\"\"\n",
    "        recommendations = analysis_result.get('recommendations', [])\n",
    "        \n",
    "        for rec in recommendations:\n",
    "            # Create bet info\n",
    "            bet_info = {\n",
    "                'match_id': f\"{rec['home_team']}_{rec['away_team']}_{int(time.time())}\",\n",
    "                'league': rec['league'],\n",
    "                'home_team': rec['home_team'],\n",
    "                'away_team': rec['away_team'],\n",
    "                'selection': rec['selection'],\n",
    "                'book_odds': rec['book_odds'],\n",
    "                'fair_odds': rec['fair_odds'],\n",
    "                'edge': rec['edge'],\n",
    "                'kelly_pct': rec['kelly_pct'],\n",
    "                'clv': rec['clv']\n",
    "            }\n",
    "            \n",
    "            # Add bet to paper trading\n",
    "            self.trader.add_bet(bet_info)\n",
    "            \n",
    "            # Format and display\n",
    "            self.display_bet_recommendation(rec)\n",
    "    \n",
    "    def display_bet_recommendation(self, rec: Dict):\n",
    "        \"\"\"Display formatted bet recommendation\"\"\"\n",
    "        stake_amount = (rec['kelly_pct'] / 100) * self.trader.bankroll\n",
    "        \n",
    "        print(\"\\n\" + \"=\"*60)\n",
    "        print(f\"üá´üá∑ {rec['league']} ‚Äì {rec['home_team']} vs {rec['away_team']}\")\n",
    "        print(f\"Bet: {rec['selection']} @{rec['book_odds']} (fair {rec['fair_odds']}) ‚Üí edge {rec['edge']:.1f}%\")\n",
    "        print(f\"Kelly: {rec['kelly_pct']:.1f}% bankroll ‚Üí ‚Ç¨{stake_amount:.0f} sur 10k\")\n",
    "        print(f\"Probability: {rec['probability']:.1%} | CLV: {rec['clv']:.1f}%\")\n",
    "        print(\"=\"*60)\n",
    "    \n",
    "    def human_delay(self):\n",
    "        \"\"\"Add human-like delay for stealth\"\"\"\n",
    "        delay = random.uniform(*CONFIG['human_delay'])\n",
    "        print(f\"‚è±Ô∏è  Human delay: {delay:.1f}s\")\n",
    "        time.sleep(delay)\n",
    "\n",
    "# Initialize main analyzer\n",
    "analyzer = GPTBetFootAnalyzer()\n",
    "print(\"üéØ GPT-Bet.Foot Analyzer Ready\")\n",
    "print(\"\\nüìã Ready to analyze matches!\")\n",
    "print(\"Usage: analyzer.analyze_match(match_data)\")"
