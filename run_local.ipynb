   "source": [
    "class OddsOCR:\n",
    "    \"\"\"OCR system for extracting odds from screenshots\"\"\"\n",
    "    \n",
    "    def __init__(self):\n",
    "        self.setup_tesseract()\n",
    "    \n",
    "    def setup_tesseract(self):\n",
    "        \"\"\"Setup Tesseract OCR\"\"\"\n",
    "        try:\n",
    "            # For Windows\n",
    "            pytesseract.pytesseract.tesseract_cmd = r'C:\\Program Files\\Tesseract-OCR\\tesseract.exe'\n",
    "        except:\n",
    "            print(\"‚ö†Ô∏è Tesseract not found. Using GPT-4 Vision instead.\")\n",
    "    \n",
    "    def extract_odds_gpt4(self, image_path: str) -> Dict[str, float]:\n",
    "        \"\"\"Use GPT-4 Vision to extract odds from image\"\"\"\n",
    "        \n",
    "        # Convert image to base64 for GPT-4 Vision\n",
    "        import base64\n",
    "        \n",
    "        with open(image_path, \"rb\") as image_file:\n",
    "            base64_image = base64.b64encode(image_file.read()).decode('utf-8')\n",
    "        \n",
    "        response = client.chat.completions.create(\n",
    "            model=\"gpt-4-turbo\",\n",
    "            messages=[\n",
    "                {\n",
    "                    \"role\": \"user\",\n",
    "                    \"content\": [\n",
    "                        {\n",
    "                            \"type\": \"text\",\n",
    "                            \"text\": \"Extract betting odds from this football betting interface. Return only a JSON object with home, draw, and away odds as decimal numbers. Focus on the main 1X2 market odds.\"\n",
    "                        },\n",
    "                        {\n",
    "                            \"type\": \"image_url\",\n",
    "                            \"image_url\": {\n",
    "                                \"url\": f\"data:image/jpeg;base64,{base64_image}\"\n",
    "                            }\n",
    "                        }\n",
    "                    ]\n",
    "                }\n",
    "            ],\n",
    "            max_tokens=100\n",
    "        )\n",
    "        \n",
    "        # Parse response\n",
    "        try:\n",
    "            content = response.choices[0].message.content\n",
    "            # Extract JSON from response\n",
    "            import re\n",
    "            json_match = re.search(r'\{[^}]+\}', content)\n",
    "            if json_match:\n",
    "                odds_dict = json.loads(json_match.group())\n",
    "                return {k: float(v) for k, v in odds_dict.items()}\n",
    "        except:\n",
    "            pass\n",
    "        \n",
    "        return {'home': 0, 'draw': 0, 'away': 0}\n",
    "    \n",
    "    def extract_odds_tesseract(self, image_path: str) -> Dict[str, float]:\n",
    "        \"\"\"Use Tesseract OCR to extract odds\"\"\"\n",
    "        try:\n",
    "            image = Image.open(image_path)\n",
    "            text = pytesseract.image_to_string(image)\n",
    "            \n",
    "            # Parse odds using regex\n",
    "            import re\n",
    "            odds_pattern = r'(\d+\.\d+)'\n",
    "            odds = re.findall(odds_pattern, text)\n",
    "            \n",
    "            if len(odds) >= 3:\n",
    "                return {\n",
    "                    'home': float(odds[0]),\n",
    "                    'draw': float(odds[1]),\n",
    "                    'away': float(odds[2])\n",
    "                }\n",
    "        except:\n",
    "            pass\n",
    "        \n",
    "        return {'home': 0, 'draw': 0, 'away': 0}\n",
    "    \n",
    "    def extract_odds(self, image_path: str, method='gpt4') -> Dict[str, float]:\n",
    "        \"\"\"Main method to extract odds from image\"\"\"\n",
    "        if method == 'gpt4':\n",
    "            return self.extract_odds_gpt4(image_path)\n",
    "        else:\n",
    "            return self.extract_odds_tesseract(image_path)\n",
    "\n",
    "# Initialize OCR system\n",
    "odds_ocr = OddsOCR()\n",
    "print(\"üì∏ OCR system ready (GPT-4 Vision + Tesseract)\")"
   ]
