    "class PaperTradingSystem:\n",
    "    \"\"\"Paper-trading system for tracking performance\"\"\"\n",
    "    \n",
    "    def __init__(self, bankroll: float = 10000):\n",
    "        self.bankroll = bankroll\n",
    "        self.initial_bankroll = bankroll\n",
    "        self.trades_file = f\"{CONFIG['data_dir']}/trades.csv\"\n",
    "        self.setup_trades_file()\n",
    "    \n",
    "    def setup_trades_file(self):\n",
    "        \"\"\"Initialize trades CSV file\"\"\"\n",
    "        if not os.path.exists(self.trades_file):\n",
    "            df = pd.DataFrame(columns=[\n",
    "                'timestamp', 'match_id', 'league', 'home_team', 'away_team',\n",
    "                'selection', 'book_odds', 'fair_odds', 'edge', 'kelly_pct',\n",
    "                'stake_amount', 'result', 'profit_loss', 'bankroll',\n",
    "                'clv', 'delta_xg', 'delta_ranking', 'delta_days'\n",
    "            ])\n",
    "            df.to_csv(self.trades_file, index=False)\n",
    "    \n",
    "    def add_bet(self, bet_info: Dict) -> bool:\n",
    "        \"\"\"Add a new bet to the tracking system\"\"\"\n",
    "        try:\n",
    "            # Calculate stake\n",
    "            kelly_pct = bet_info['kelly_pct']\n",
    "            stake_amount = (kelly_pct / 100) * self.bankroll\n",
    "            \n",
    "            # Ensure we don't bet more than available\n",
    "            stake_amount = min(stake_amount, self.bankroll * 0.05)  # Max 5% per bet\n",
    "            \n",
    "            # Create trade record\n",
    "            trade = {\n",
    "                'timestamp': datetime.now().isoformat(),\n",
    "                'match_id': bet_info.get('match_id', f\"match_{int(time.time())}\"),\n",
    "                'league': bet_info['league'],\n",
    "                'home_team': bet_info['home_team'],\n",
    "                'away_team': bet_info['away_team'],\n",
    "                'selection': bet_info['selection'],\n",
    "                'book_odds': bet_info['book_odds'],\n",
    "                'fair_odds': bet_info['fair_odds'],\n",
    "                'edge': bet_info['edge'],\n",
    "                'kelly_pct': kelly_pct,\n",
    "                'stake_amount': stake_amount,\n",
    "                'result': 'pending',\n",
    "                'profit_loss': 0,\n",
    "                'bankroll': self.bankroll,\n",
    "                'clv': bet_info.get('clv', 0),\n",
    "                'delta_xg': bet_info.get('delta_xg', 0),\n",
    "                'delta_ranking': bet_info.get('delta_ranking', 0),\n",
    "                'delta_days': bet_info.get('delta_days', 0)\n",
    "            }\n",
    "            \n",
    "            # Add to CSV\n",
    "            df = pd.read_csv(self.trades_file)\n",
    "            df = pd.concat([df, pd.DataFrame([trade])], ignore_index=True)\n",
    "            df.to_csv(self.trades_file, index=False)\n",
    "            \n",
    "            # Update bankroll (subtract stake)\n",
    "            self.bankroll -= stake_amount\n",
    "            \n",
    "            print(f\"‚úÖ Bet added: {bet_info['home_team']} vs {bet_info['away_team']}\")\n",
    "            print(f\"   Stake: ‚Ç¨{stake_amount:.2f} ({kelly_pct:.1f}% Kelly)\")\n",
    "            print(f\"   Edge: {bet_info['edge']:.1f}% | Bankroll: ‚Ç¨{self.bankroll:.2f}\")\n",
",
    "            return True\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Error adding bet: {e}\")\n",
    "            return False\n",
    "    \n",
    "    def settle_bet(self, match_id: str, result: str, actual_result: str):\n",
    "        \"\"\"Settle a bet with actual result\"\"\"\n",
    "        try:\n",
    "            df = pd.read_csv(self.trades_file)\n",
    "            \n",
    "            # Find the bet\n",
    "            bet_idx = df[df['match_id'] == match_id].index\n",
    "            if len(bet_idx) == 0:\n",
    "                print(f\"‚ùå Bet not found: {match_id}\")\n",
    "                return\n",
    "            \n",
    "            idx = bet_idx[0]\n",
    "            \n",
    "            # Calculate profit/loss\n",
    "            if df.loc[idx, 'selection'] == result:\n",
    "                profit = df.loc[idx, 'stake_amount'] * (df.loc[idx, 'book_odds'] - 1)\n",
    "            else:\n",
    "                profit = -df.loc[idx, 'stake_amount']\n",
    "            \n",
    "            # Update record\n",
    "            df.loc[idx, 'result'] = result\n",
    "            df.loc[idx, 'profit_loss'] = profit\n",
    "            \n",
    "            # Update bankroll\n",
    "            self.bankroll += df.loc[idx, 'stake_amount'] + profit\n",
    "            df.loc[idx, 'bankroll'] = self.bankroll\n",
    "            \n",
    "            # Save\n",
    "            df.to_csv(self.trades_file, index=False)\n",
    "            \n",
    "            outcome = \"WIN\" if profit > 0 else \"LOSS\"\n",
    "            print(f\"‚úÖ {outcome}: {match_id} | Profit: ‚Ç¨{profit:.2f}\")\n",
    "            print(f\"   New Bankroll: ‚Ç¨{self.bankroll:.2f}\")\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Error settling bet: {e}\")\n",
    "    \n",
    "    def get_performance_stats(self) -> Dict:\n",
    "        \"\"\"Calculate performance statistics\"\"\"\n",
    "        try:\n",
    "            df = pd.read_csv(self.trades_file)\n",
    "            settled_bets = df[df['result'] != 'pending']\n",
    "            \n",
    "            if len(settled_bets) == 0:\n",
    "                return {'message': 'No settled bets yet'}\n",
    "            \n",
    "            total_bets = len(settled_bets)\n",
    "            wins = len(settled_bets[settled_bets['profit_loss'] > 0])\n",
    "            losses = len(settled_bets[settled_bets['profit_loss'] < 0])\n",
    "            \n",
    "            total_profit = settled_bets['profit_loss'].sum()\n",
    "            total_staked = settled_bets['stake_amount'].sum()\n",
    "            \n",
    "            roi = (total_profit / total_staked * 100) if total_staked > 0 else 0\n",
    "            win_rate = (wins / total_bets * 100) if total_bets > 0 else 0\n",
    "            \n",
    "            # Calculate yield\n",
    "            yield_pct = (total_profit / total_staked * 100) if total_staked > 0 else 0\n",
    "            \n",
    "            # Calculate CLV (Closing Line Value)\n",
    "            avg_clv = settled_bets['clv'].mean() if 'clv' in settled_bets.columns else 0\n",
    "            \n",
    "            # Calculate max drawdown\n",
    "            bankroll_history = settled_bets['bankroll'].values\n",
    "            peak = bankroll_history[0]\n",
    "            max_dd = 0\n",
    "            \n",
    "            for value in bankroll_history[1:]:\n",
    "                if value > peak:\n",
    "                    peak = value\n",
    "                else:\n",
    "                    dd = (peak - value) / peak * 100\n",
    "                    max_dd = max(max_dd, dd)\n",
    "            \n",
    "            return {\n",
    "                'total_bets': total_bets,\n",
    "                'wins': wins,\n",
    "                'losses': losses,\n",
    "                'win_rate': round(win_rate, 2),\n",
    "                'total_profit': round(total_profit, 2),\n",
    "                'total_staked': round(total_staked, 2),\n",
    "                'roi': round(roi, 2),\n",
    "                'yield': round(yield_pct, 2),\n",
    "                'clv': round(avg_clv, 2),\n",
    "                'max_drawdown': round(max_dd, 2),\n",
    "                'current_bankroll': round(self.bankroll, 2),\n",
    "                'profit_vs_initial': round(self.bankroll - self.initial_bankroll, 2)\n",
    "            }\n",
    "            \n",
    "        except Exception as e:\n",
    "            print(f\"‚ùå Error calculating stats: {e}\")\n",
    "            return {}\n",
    "\n",
    "# Initialize paper trading system\n",
    "paper_trader = PaperTradingSystem(CONFIG['bankroll'])\n",
    "print(\"üìä Paper-trading system initialized\")\n",
    "print(f\"üí∞ Initial bankroll: ‚Ç¨{paper_trader.bankroll:,}\")"
   ]
